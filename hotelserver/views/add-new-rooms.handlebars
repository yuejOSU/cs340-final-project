<body style="background-color:#FFFAED; text-align:center">
  <h1>Add New Rooms</h1>
    <div class="create-container">
        <form>
            <div>
                <div class="inputs">

                    <div class="field">
                        <label>Cleaned</label>
                        <input name="is_clean" type="text" placeholder="TRUE or FALSE">
                    </div>

                    <div class="field">
                        <label>Occupied</label>
                        <input name="is_occupied" type="text" required="required" placeholder="TRUE or FALSE"
                    </div>

                </div>

                <input type="submit" value="Add">
            </div>
        </form>
    </div>
    <div>
        <table>
            <thead>
                <th>Room Number</th>
                <th>Clean</th>
                <th>Vacant</th>
            </thead>
            {{#each results}}
              <tr>
                <td>{{room_id}}</td>
                <td>{{is_clean}}</td>
                <td>{{is_occupied}}</td>
              </tr>
            {{/each}}
      </div>
