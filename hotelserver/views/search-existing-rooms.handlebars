<body style="background-color:#FFFAED; text-align:center">
  <h1>Search Existing Rooms</h1>
    <div class="create-container">
        <form>
            <div>
                <div class="inputs">
                    <div class="field">
                        <label>Room number</label>
                        <input name="room_number" type="text" required="required" placeholder="Room Number"
                    </div>

                </div>

                <input type="submit" value="Search">
            </div>
        </form>
    </div>

    <div>
        <table>
            <thead>
                <th>Room Number</th>
                <th>Clean</th>
                <th>Vacant</th>
                <th></th>
                <th></th>
            </thead>
            {{#each results}}
              <tr>
                <td>{{room_id}}</td>
                <td>{{is_clean}}</td>
                <td>{{is_occupied}}</td>
                <td><form action="/edit" method="post"><input type="submit" value="Edit" onclick="editRow({{this.id}})"></form></td>
				<td><input type="submit" value="Delete" onclick="deleteRow({{this.id}})"></td>
              </tr>
            {{/each}}
      </div>
